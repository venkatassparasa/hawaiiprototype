{
  "name": "hawaii-vrbo-airbnb-crawler-api",
  "version": "1.0.0",
  "description": "API service for crawling Airbnb and VRBO listings to detect TVR violations in Hawaii County",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "worker": "node dist/worker.js",
    "worker:dev": "nodemon src/worker.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t hawaii-vr-crawler .",
    "docker:run": "docker run -p 3000:3000 hawaii-vr-crawler"
  },
  "keywords": [
    "airbnb",
    "vrbo",
    "crawler",
    "hawaii",
    "tvr",
    "violation-detection",
    "apify",
    "compliance"
  ],
  "author": "Hawaii County Government",
  "license": "MIT",
  "dependencies": {
    "@temporalio/activity": "^1.14.1",
    "@temporalio/client": "^1.14.1",
    "@temporalio/common": "^1.14.1",
    "@temporalio/worker": "^1.14.1",
    "@temporalio/workflow": "^1.14.1",
    "@types/pg": "^8.16.0",
    "apify": "^3.1.10",
    "aws-sdk": "^2.1520.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
